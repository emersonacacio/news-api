{
  "git.ignoreLimitWarning": true,
  "workbench.colorTheme": "Winter is Coming (Dark Blue - No Italics)",
  "editor.formatOnSave": true,
  "editor.fontSize": 14,
  "editor.fontFamily": "JetBrains Mono",
  "auto-rename-tag.activationOnLanguage": [
    "html",
    "xml",
    "php",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "javascript.suggest.autoImports": true,
  "extension.changeCase.commands": true,
  "styledComponentsExtractor.addImportStatement": true,
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "connectString": "postgres://usbwbd_integrispan:postgrespassword@localhost:5432/sbwbd",
      "driver": "PostgreSQL",
      "name": "IntegriSpan"
    }
  ],
  "cSpell.words": [
    "antd",
    "Cascader",
    "CHOKIDAR",
    "dgraph",
    "dind",
    "gitlab",
    "grahpql",
    "hasura",
    "Inps",
    "Inpusts",
    "Integri",
    "Integridutos",
    "INTEGRIDUTOS",
    "integrispan",
    "Plotly",
    "postgres",
    "rrdls",
    "subweb",
    "USEPOLLING",
    "uuidv",
    "WEVERTONM"
  ],

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "npm-intellisense.importES6": true,
  "npm-intellisense.importQuotes": "'",
  "npm-intellisense.importLinebreak": ";\r\n",
  "npm-intellisense.importDeclarationType": "const",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  }
}
